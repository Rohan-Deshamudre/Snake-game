<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="gui.controller.LoginController">
    <children>
        <ImageView fitHeight="605.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../image/bricks.jpg"/>
            </image>
        </ImageView>
        <ImageView fitHeight="254.0" fitWidth="213.0" layoutX="68.0" layoutY="178.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../image/transparent_logo.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="54.0" fitWidth="284.0" layoutX="434.0" layoutY="94.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../image/register_t.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="55.0" fitWidth="213.0" layoutX="341.0" layoutY="197.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../image/username_t.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="54.0" fitWidth="200.0" layoutX="341.0" layoutY="252.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../image/password_t.png"/>
            </image>
        </ImageView>
        <!--        <Button layoutX="364.0" layoutY="353.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="181.0" styleClass="button1" stylesheets="@../css/login.css" />-->
        <Button layoutX="606.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#register" prefHeight="45.0"
                prefWidth="182.0" styleClass="button1" stylesheets="@../css/login.css">
            <graphic>
                <ImageView fitHeight="57.0" fitWidth="264.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../image/register_t.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <TextField fx:id="registerUsername" layoutX="688.0" layoutY="198.0" prefHeight="34.0" prefWidth="192.0"
                   stylesheets="@../css/login.css"/>
        <ImageView fitHeight="34.0" fitWidth="284.0" layoutX="349.0" layoutY="314.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../image/confirm_t.png"/>
            </image>
        </ImageView>
        <Button layoutX="349.0" layoutY="422.0" mnemonicParsing="false" onMouseClicked="#goBackLogin"
                stylesheets="@../css/entryStyle.css">
            <graphic>
                <ImageView fitHeight="41.0" fitWidth="92.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../image/arrow.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <PasswordField fx:id="registerPassword" layoutX="688.0" layoutY="256.0" prefHeight="34.0" prefWidth="192.0"/>
        <PasswordField fx:id="confirmPassword" layoutX="688.0" layoutY="312.0" prefHeight="34.0" prefWidth="192.0"/>
    </children>
</AnchorPane>
